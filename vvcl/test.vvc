append_zero = (str: String) -> String {
    (str ~ "0")
};

main = (a: Bool, b: Int) -> Option<Record> {
    ? Option::Some` 5 | Some` x => < foo = (x * b); > | None` => Option::None`
};

rec = (a: Int) -> Record {
    < x = 5; foo = (a*7); >
};

add_two = (a: Int) -> Int {
    added = { (0 + 2 )};
    (a + added)
};

addfour = (value: Int) -> Int {
    (add_two(value) + 2)
};
