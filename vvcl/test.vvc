append_zero = (str: String) -> String {
    (str ~ "0")
};

main = (a: Int, b: Int) -> Int {
    prod = (a * b);
    list_map(list_map([1,2,(a+b), prod], int_to_str), append_zero)
};

rec = (a: Int) -> Record {
    < x = 5; foo = (a*7); >
};

add_two = (a: Int) -> Int {
    added = { (0 + 2 )};
    (a + added)
};

addfour = (value: Int) -> Int {
    (add_two(value) + 2)
};
